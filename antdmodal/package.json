{
  "name": "antdmodal",
  "version": "1.0.0",
  "description": "iCat-react-antd demo",
  "main": "dist/index.js",
  "entry": {
    "index": "./src/entry/index.jsx"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "dora -p 8002 --plugins atool-build,proxy,hmr",
