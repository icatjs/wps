  "license": "MIT",
  "devDependencies": {
    "antd": "~0.10.4",
    "atool-build": "~0.4.2",
    "babel-plugin-antd": "~0.1.3",
    "babel-plugin-react-originjs": "0.0.4",
    "dora": "^0.1.1",
    "dora-plugin-atool-build": "^0.2.1",
    "dora-plugin-hmr": "^0.1.4",
    "dora-plugin-proxy": "^0.4.0",
    "react": "~0.14.3",
    "react-dom": "~0.14.3"
  }
}
